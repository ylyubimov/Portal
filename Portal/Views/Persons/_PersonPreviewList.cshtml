<div class=" container person-wrap">
    @if (ViewBag.Type != "students")
    {
        <div class="person-part-name">Преподаватели</div>
        foreach (Portal.Models.Person person in Model[0])
        {
            @Html.Partial("_PersonPreview", person)
        }
    }
    @if (ViewBag.Type != "teachers")
    {
        <div class="person-part-name">Студенты</div>
        foreach (Portal.Models.Person person in Model[1])
        {
            @Html.Partial("_PersonPreview", person)
        }
    }
</div>

