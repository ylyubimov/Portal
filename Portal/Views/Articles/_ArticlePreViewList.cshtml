@if (Model.Length != 0)
{
    foreach (var i in Model)
    {
       @Html.Partial("../Articles/_ArticlePreView", (Portal.Models.Article)i)
    }
}
else
{
    <div class="container search-error-wrap">
        <div class="search-error">
            К сожалению, по вашему запросу ничего не найдено
        </div>
    </div>
}