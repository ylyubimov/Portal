@if (Model.Length != 0)
{
    foreach (var i in Model)
    {
       @Html.Partial("../Articles/_ArticlePreView", (Portal.Models.Article)i)
    }
}
else
{
    <div class="container article-wrap">
        <div class="article-preview">
            <h1> Ничего не найдено</h1>
        </div>
    </div>
}